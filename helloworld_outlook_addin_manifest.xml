<?xml version="1.0" encoding="UTF-8"?>
<OfficeAddin
  xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:type="MailApp">

  <Id>N7y1Vh6bzU2NfPUCz5ORVg==</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>BBAK Getting Things Done</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="BBAK_GTD"/>
  <Description DefaultValue="BBAK's implementation of GTD"/>
  <IconUrl DefaultValue="https://assldev.github.io/outlook_addin_gtd/assets/Logo2.png" />
  <HighResolutionIconUrl DefaultValue="https://assldev.github.io/outlook_addin_gtd/assets/Logo2.png" />
  
  <!-- Permissions -->
  <Hosts>
    <Host Name="Mailbox" />
  </Hosts>
  
  <!-- Online or Offline -->
  <Supports>
    <SupportsItem>
      <SupportsFormFactor Name="MessageRead"/>
    </SupportsItem>
  </Supports>

  <!-- Functionality -->
  <FormSettings>
    <Form xsi:type="ItemRead">
      <DesktopSettings>
        <SourceLocation DefaultValue="https://assldev.github.io/outlook_addin_gtd/message-read.html" />
        <RequestedHeight>250</RequestedHeight>
      </DesktopSettings>
    </Form>
  </FormSettings>

</OfficeAddin>